$("#orderTable").html("<%= escape_javascript (render partial: 'table', :locals => { :orders => @orders }) %>");
<% if params[:search].present? %>
  $("#searchedOrders").text("Searched Orders")
<% end %>

$("tr[data-link]").click(function() {
  window.location = $(this).data("link")
})
<% if params[:order_ids].present? %>
  window.print();
<% end %>
